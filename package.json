{
  "name": "nextjs_ci",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:all": "npm run style:lint && npm run style:prettier",
    "style:lint": "eslint **/*.{js,jsx,ts,tsx}",
