  },
  "dependencies": {
    "next": "13.0.0",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "eslint-plugin-unused-imports": "^2.0.0"
  }
}
